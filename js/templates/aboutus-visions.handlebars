 <h3 class="text-dark">Our Vision</h3>
        {{#each data}}     
        <div class="row">
            <div class="col-sm-4">
                <a href="#" onclick="loadData('visions/{{id}}','#content','aboutus-details')">
                    <img class="round-img" src="{{thumbpath}}" alt="">         
                </a>              
            </div>
            <div class="col-sm-8">
                <a href="#" onclick="loadData('visions/{{id}}','#content','aboutus-details')">
                    <h4 class="py-0 my-0">{{name}}</h4>
                </a>
                <p>{{summary}}</p>
            </div>
        </div>      
        {{/each}}